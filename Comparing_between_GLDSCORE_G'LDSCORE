library(corrplot)
library(dplyr)
setwd("../00_block0")
g <- read.table("v8.merge",header=T)
bim <- read.table("../block0.bim")
names(g) <- bim$V2
ldscore <- cor(g)
ldscore2 <- ldscore*ldscore
ldscore3 <- data.frame(rowSums(ldscore2))
gctaldscore <- read.table("../block0.score.ld",header=T)
cor.test(gctaldscore$ldscore,ldscore3$rowSums.ldscore2.)
